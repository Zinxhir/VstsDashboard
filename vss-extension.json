{
  "manifestVersion": 1,
  "id": "cicd-dashboard",
  "publisher": "experta",
  "version": "0.0.0",
  "name": "CICDDashboard",
  "description": "Track events important to your team, view and manage days off, quickly see when sprints start and end, and more.",
  "categories": ["Azure Pipelines"],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "scopes": [
    "vso.project",
    "vso.build", 
    "vso.release"
  ],
  "icons": {
    "default": "icon.png"
  },
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "links": {
    "support": {
      "uri": "https://github.com/expertasolutions/VstsDashboard/issues"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/expertasolutions/VstsDashboard"
  },
  "files": [
    {
      "path":"dist",
      "addressable": true
    },
    {
      "path":"static",
      "addressable": true
    },
    {
      "path":"node_modules/vss-web-extension-sdk/lib",
      "addressable":true,
      "packagePath":"lib"
    }
  ],
  "contributions": [
    {
      "id":"cicdpipelines",
      "type":"ms.vss-web.hub",
      "description": "CI/CD Pipelines in the Work Hub group",
      "targets":[
        "ms.vss-build-web.build-release-hub-group",
        "ms.vss-releaseManagement-web.release-service-data-external"
      ],
      "properties": {
        "name":"CI/CD Dashboard",
        "order":100,
        "uri":"static/index.html"
      }
    }
  ]
}